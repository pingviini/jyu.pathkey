<?xml version="1.0"?>
<object name="portal_actions" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="document_actions">
  <object name="add_pathkey" meta_type="CMF Action"
     i18n:domain="jyu.pathkey">
   <property name="title" i18n:translate="">Add pathkey</property>
   <property name="description" i18n:translate="">Restricts access to content without proper path key.</property>
   <property name="url_expr">string:${object_url}/pathkey-form</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python: checkPermission("Modify portal content", object)</property>
   <property name="permissions">
	<element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
